<div class= "col-md-8 offset-md-1">
<h2>ユーザー</h2>
<table class= "table table-hover table-inverse">
  <thead>
    <tr>
    <th>プロフィール写真</th>
    <th>ニックネーム</th>
    <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    　<tr>
        <td>
        　<%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpeg', fallback: "no_image.jpg", size: "300x250" %>
        </td>
        <td><%= user.name %></td>
        <%= link_to "退会させる ", withdraw_path(user.id), method: :delete, class: "btn btn-danger" %>
        <% end %>
  </tbody>
</table>
</div>