<div class= "col-md-8 offset-md-1">
<h2>ユーザー</h2>
<table class= "table table-hover table-inverse">
  <thead>
    <tr>
    <th>プロフィール写真</th>
    <th>ニックネーム</th>
    <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
     <tr>
        <td>
         <%= attachment_image_tag user, :profile_image, fallback: "no_image.jpg", size: "300x250" %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to "退会させる ", withdraw_path(user.id), method: :delete, data:{ confirm: '本当に消しますか？' }, class: "btn btn-danger" %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>